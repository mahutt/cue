{
    "name": "341bones",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run build-client && node ./bin/www",
        "build-client": "cd client && npm run build",
        "dev": "concurrently \"nodemon ./bin/www\" \"cd client && npm run dev\"",
        "dev-server": "nodemon ./bin/www",
        "backup": "cp ./database/cue.db ./backups/cue-$(date +%Y-%m-%d-%H-%M-%S).db"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "bootstrap-icons": "^1.11.3",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^16.4.4",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-async-handler": "^1.2.0",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.1.0",
        "morgan": "~1.9.1",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "concurrently": "^9.0.1",
        "nodemon": "^3.0.3"
    }
}
